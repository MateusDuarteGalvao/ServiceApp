            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.PerfilPrestadorActivity"
            android:parentActivityName=".activity.PrestadorActivityDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>