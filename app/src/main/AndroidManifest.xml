<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.duarte.serviceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logoe"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.OrdensServicoClienteActivity"
            android:parentActivityName=".activity.PrestadorActivityDrawer"
            android:screenOrientation="portrait" />
        <activity android:name=".activity.SuporteActivityPrestador" />
        <activity android:name=".activity.PrestadorActivityDrawer" />
        <activity android:name=".activity.HomeActivityDrawer" />
        <activity android:name=".activity.SuporteActivity" />
        <activity
            android:name=".activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SelecaoUsuarioActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CadastroPrestadorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.CadastroClienteActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.SliderActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.OrdensServicoActivity"
            android:parentActivityName=".activity.PrestadorActivityDrawer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".activity.ServicosActivity"
